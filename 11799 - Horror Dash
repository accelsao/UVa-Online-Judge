using namespace std;

int main() {
	int n;
	int t;
	cin >> t;
	for (int i = 1; i <= t;i++) {
		cin >> n;
		int s = -1,m;
		while (n--)cin >> m, s = max(s, m);
		printf("Case %d: %d\n", t, s);
	}
}
