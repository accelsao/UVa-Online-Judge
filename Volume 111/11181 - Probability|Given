#include<iostream>
#include<cmath>
#include<algorithm>
#include<cstring>
#include<cstdio>
#include<cstdlib>
#include<queue>
#include<string>
#include<vector>
using namespace std;

double p[21];
double m[1 << 21];
double ans[21];
int main(){
	int n, r;
	int t = 1;
	while (cin >> n >> r, n){
		double sum = 0;
		for (int i = 1; i <= n; i++)
			cin >> p[i],ans[i]=0;

		for (int i = 0; i < (1 << n); i++){
	
			int k = i, d = 0;
			while (k){
				if (k & 1)
					d++;
				k >>= 1;		
			}
			if (d == r){
				double s = 1;
				k = i, d = 1;
				while (d <= n){
					if (k & 1)s *= p[d];
					else s *= 1 - p[d];
					d++, k >>= 1;
				}
				k = i, d = 1;
				while (k){
					if (k & 1)ans[d] += s;
					d++, k >>= 1;
				}
				sum += s;
			}
				
		}
		printf("Case %d:\n", t++);

		int s = 1;
		while (s <= n){
			printf("%.6lf\n", ans[s] / sum);
			s++;
		}
	}
}
