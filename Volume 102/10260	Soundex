#include<iostream>
#include<algorithm>
#include<map>
#include<string>
#include<cstdio>
#include<queue>
#include<cmath>
#include<stack>
#include<sstream>
using namespace std;

int s[26] = {};

int main(){
	string c;
	while (cin >> c){
		s[1] = s['F' - 'A'] = s['P' - 'A'] = s['V' - 'A'] = 1;
		s['C' - 'A'] = s['G' - 'A'] = s['J' - 'A'] = s['J' - 'A'] = s['K' - 'A'] = s['Q' - 'A'] = s['S' - 'A'] = s['X' - 'A'] = s['Z' - 'A'] = 2;
		s['D' - 'A'] = s['T' - 'A'] = 3;
		s['L' - 'A'] = 4;
		s['M' - 'A'] = s['N' - 'A'] = 5;
		s['R' - 'A'] = 6;
		int lc =0 ;
		for (int i = 0; i < c.size(); i++){
			if (s[c[i]-'A'] == lc)continue;
			if (s[c[i] - 'A']){ cout << s[c[i] - 'A'];  }
			lc = s[c[i] - 'A'];
		}
		cout << endl;
	}
}
