#include<iostream>
#include<algorithm>
#include<map>
#include<string>
#include<cstdio>
#include<queue>
#include<cmath>
#include<stack>
#include<sstream>
using namespace std;

int main(){
	int n;
	while (cin >> n,n){
		int ans = 0;
		int sum = 0;
		int a;
		for (int i = 0; i < n; i++){
			cin >> a;
			sum += a;
			if (sum < 0)
				sum = 0;
			else{
				ans = max(ans, sum);
			}
		}
		if (ans>0)
			printf("The maximum winning streak is %d.\n", ans);
		else
			printf("Losing streak.\n");
	}
}
