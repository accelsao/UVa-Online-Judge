#include<iostream>
#include<algorithm>
#include<map>
#include<string>
#include<cstdio>
#include<queue>
#include<cmath>
#include<stack>
#include<cctype>
#include<sstream>
using namespace std;

int main(){
	int m, n;
	int t;

	while (cin >> m>>n >> t){
		if (m > n)swap(m, n);
		int time[10001] = {};//time[i]=k i分鐘可用到k分鐘
		int num[10001] = {};//num[i]=k i分鐘可吃到k個
		for (int i = 0; i + m <= t; i++)
			if (time[i + m] < time[i] + m){
				time[i + m] = time[i] + m;
				num[i + m] = num[i] + 1;
			}
		for (int i = 0; i + n <= t; i++)
			if (time[i + n] < time[i] + n){
				time[i + n] = time[i] + n;
				num[i + n] = num[i] + 1;
			}
		if (time[t] == t)printf("%d\n", num[t]);
		else printf("%d %d\n", num[time[t]], t - time[t]);
	}
}
