#include<iostream>
#include<sstream>
#include<algorithm>
#include<iomanip>
#include<cstdio>
#include<cstdlib>
#include<cctype>
#include<map>
#include<vector>
#include<cmath>
#include<queue>
#include<cstring>
#include<set>
#include<functional>
#include<list>
#include<stack>
#include<cstring>
#include<utility>
#include <climits>

using namespace std;
#define printf printf_s
#define sprintf sprintf_s
#define sscanf sscanf_s
#define scanf scanf_s

int node[400010] = {};
int n;
void update(int m, int x){
	while (m <= n){
		node[m] += x;
		m += m&(-m);
	}
}
int query(int x){
	int res = 0;
	while (x){
		res += node[x];
		x -= x&(-x);
	}
	return res;
}
	int tt = 0;
int main(){
	int p;
	while (cin >> n,n){
		tt++;
		for (int i = 1; i <= n; i++){ node[i] = 0; }
		for (int i = 1; i <= n; i++){
			
			cin >> p;
			update(i,p);
		}
		if (tt != 1)puts("");
		printf("Case %d:\n", tt);
		string s;
		int a, b;
		while (cin >> s, s != "END"){
			cin >> a >> b;
			if (s == "M"){	
				cout << query(b) - query(a - 1) << endl;
			}
			else{
				b -= query(a)-query(a-1);
				update(a, b);
			}
		}
	}
}
