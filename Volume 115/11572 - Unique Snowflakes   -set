#include<string>
#include<cstdio>
#include<iostream>
#include<queue>
#include<cmath>
#include<stack>
#include<cctype>
#include<cstring>
#include<sstream>
#include<set>
#include<map>
#include<algorithm>
#include<functional>
#include <regex>
using namespace std;

vector<long long>p;
int main(){
	int t, n;
	cin >> t;
	while (t--){
		p.clear();
		cin >> n;
		long long k;
		for (int i = 0; i < n; i++){
			cin >> k;
			p.push_back(k);
		}
		set<long long>s;
		int l = 0, r = 0, ans = 0;
		while (r < n){
			while (r < n&&!s.count(p[r]))
				s.insert(p[r++]);
			ans = max(ans, r - l);
			s.erase(p[l++]);
		} 
		printf("%d\n", ans);
	}
}
